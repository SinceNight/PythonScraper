    "import requests\n",
    "url =\"https://fanyi.baidu.com/sug\"   #这里sug是返回\n",
    "\n",
    "s = input(\"请输入要翻译的单词\")\n",
    "\n",
    "dat = {\"kw\":s}\n",
    "resp = requests.post(url,data = dat)   # get关键词放到url；post关键词放到请求体\n",
    "print(resp.json()) # 将服务器返回的内容处理为json\n",
    "\n",
    "resp.close()"
   ]
